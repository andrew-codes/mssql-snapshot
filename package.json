{
  "name": "mssql-snapshot",
  "version": "0.0.1",
  "description": "An npm package for creating and restoring mssql snapshots.",
  "main": "index.js",
  "scripts": {
    "test": "./node_modules/.bin/mocha --compilers js:babel-core/register --recursive",
    "compile": "./node_modules/.bin/babel -d lib/ src/",
    "watch": "./node_modules/.bin/babel -d -w lib/ --out-dir src/",
    "prepublish": "npm run compile"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/codeEmitter/mssql-snapshot.git"
  },
  "keywords": [
    "sql",
    "mssql",
    "snapshot",
    "backup"
  ],
  "author": "codeEmitter",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/codeEmitter/mssql-snapshot/issues"
  },
  "homepage": "https://github.com/codeEmitter/mssql-snapshot#readme",
  "devDependencies": {
    "babel-cli": "6.11.4",
    "babel-core": "6.13.2",
    "babel-polyfill": "6.3.14",
    "babel-preset-es2015": "6.13.2",
    "chai": "3.5.0",
    "chai-as-promised": "6.0.0",
    "mocha": "3.2.0"
  },
  "dependencies": {
    "mssql": "3.3.0"
  }
}
