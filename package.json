{
  "name": "mssql-snapshot",
  "version": "0.2.2",
  "description": "An npm package for managing mssql snapshots.",
  "main": "./lib/MssqlSnapshot.js",
  "scripts": {
    "test": "./node_modules/.bin/mocha --compilers js:babel-core/register --recursive",
    "clean": "rm -rf ./lib",
    "compile": "./node_modules/.bin/babel -d lib/ src/ && cp -r src/queries lib/queries",
    "watch": "./node_modules/.bin/babel -d -w src/ --out-dir lib/",
    "prepublish": "npm run clean && npm run compile && npm run test"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/codeEmitter/mssql-snapshot.git"
  },
  "keywords": [
    "sql",
    "mssql",
    "snapshot",
    "backup"
  ],
  "author": "codeEmitter",
  "license": "MIT",
  "bugs": {
    "url": "https://trello.com/b/03NEIEfs/mssql-snapshot"
  },
  "homepage": "https://github.com/codeEmitter/mssql-snapshot#readme",
  "devDependencies": {
    "babel-cli": "6.11.4",
    "babel-core": "6.13.2",
    "babel-polyfill": "6.3.14",
    "babel-preset-es2015": "6.13.2",
    "chai": "3.5.0",
    "mocha": "3.2.0"
  },
  "dependencies": {
    "seriate": "0.8.1"
  }
}
